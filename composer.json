{
    "name": "robmachado/restnfe",
    "type": "application",
    "description": "restnfe library",
    "homepage": "https://github.com/robmachado/restnfe",
    "keywords": ["nfe", "Restful"],
    "license": ["LGPLv3+", "GPLv3+", "MIT"],
    "authors": [
        {
            "name": "Roberto L. Machado",
            "email": "linux.rlm@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5.9|~7.0",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "nfephp-org/nfephp": "4.0.*"
    },
    "require-dev": {
	"phpunit/phpunit": "4.*",
        "fzaninotto/faker": "~1.4",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
          "RestNFe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RestNFe\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
